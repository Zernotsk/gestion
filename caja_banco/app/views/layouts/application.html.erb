<!DOCTYPE html>
<html>
<head>
  <title>CajaBanco</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag  'rails.validations.simple_form' %>
  <%= csrf_meta_tags %>
  <link href='https://fonts.googleapis.com/css?family=Josefin+Sans:400,400italic,700,600,300,300italic' rel='stylesheet' type='text/css'>
   <link href='http://fonts.googleapis.com/css?family=Open+Sans:400,300,600,700' rel='stylesheet' type='text/css'>
</head>
<body>

<%= render 'layouts/navbar' %>
<%= render 'layouts/subnavbar' %>
	<div class="container-fluid">
		<div class="row">
			<div class="container">
				<%= yield %>
			</div>
		</div>
	</div>

</body>
</html>
